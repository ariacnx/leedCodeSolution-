class Solution {
    public void reverseString(char[] s) {
        int length = s.length -1;
        int j = s.length -1;
        char temp = 'a';
        
        for(int i = 0; j > i; i++)
        {
            temp = s[i];
            s[i] = s[j];
            s[j] = temp;
            j--;
        }
        
    }
}

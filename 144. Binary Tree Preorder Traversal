class Solution {
    public List<Integer> preorderTraversal(TreeNode root) {
        List<Integer> ans = new LinkedList<Integer>();
	      preHelper(root,ans);
	      return ans;
    }
    public void preHelper(TreeNode root, List<Integer> ans) {
	    if(root==null) return;
	    ans.add(root.val);
	    preHelper(root.left,ans);
	    preHelper(root.right,ans);
    }

}
